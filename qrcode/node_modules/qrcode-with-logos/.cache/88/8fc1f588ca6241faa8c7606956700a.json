urce":"src/toImage.ts","name":null,"original":{"line":56,"column":9},"generated":{"line":243,"column":3}},{"source":"src/toImage.ts","name":null,"original":{"line":56,"column":2},"generated":{"line":243,"column":4}},{"source":"src/toImage.ts","name":null,"original":{"line":68,"column":1},"generated":{"line":244,"column":0}},{"source":"src/toImage.ts","name":null,"original":{"line":52,"column":7},"generated":{"line":244,"column":1}}],"sources":{"src/toImage.ts":"/*\r\n * @Author: super\r\n * @Date: 2019-06-27 16:29:39\r\n * @Last Modified by: super\r\n * @Last Modified time: 2019-06-27 16:46:46\r\n */\r\nimport { BaseOptions, Logo } from './model'\r\nimport { toCanvas } from './toCanvas'\r\nimport { isFunction, isString } from './utils'\r\nimport QrCodeWithLogo from '.'\r\n\r\nexport const toImage = async function (options: BaseOptions, instance: QrCodeWithLogo) {\r\n  const { canvas } = options\r\n  if (options.logo) {\r\n    if (isString(options.logo)) {\r\n      options.logo = { src: options.logo } as Logo\r\n    }\r\n    ;(options.logo as Logo).crossOrigin = 'Anonymous'\r\n  }\r\n\r\n  if (!instance.ifCanvasDrawed) await toCanvas(options)\r\n\r\n  const { image, downloadName = 'qr-code' } = options\r\n  let { download } = options\r\n\r\n  if (canvas!.toDataURL()) {\r\n    image!.src = canvas!.toDataURL()\r\n  } else {\r\n    throw new Error('Can not get the canvas DataURL')\r\n  }\r\n\r\n  instance.ifImageCreated = true\r\n\r\n  if (download !== true && !isFunction(download)) {\r\n    return\r\n  }\r\n\r\n  download = download === true ? (start: Function): Promise<void> => start() : download\r\n\r\n  const startDownload = () => {\r\n    return saveImage(image!, downloadName)\r\n  }\r\n\r\n  if (download) {\r\n    return download(startDownload)\r\n  }\r\n\r\n  return Promise.resolve()\r\n}\r\n\r\n/**save image */\r\nexport const saveImage = (\r\n  image: HTMLImageElement,\r\n  name: string\r\n): Promise<boolean> => {\r\n  return new Promise((resolve, reject) => {\r\n    try {\r\n      const dataURL = image.src\r\n      const link = document.createElement('a')\r\n      link.download = name\r\n      link.href = dataURL\r\n      link.dispatchEvent(new MouseEvent('click'))\r\n      resolve(true)\r\n    } catch (err) {\r\n      reject(err)\r\n    }\r\n  })\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"2da9c72ad7b0bd485c7135f887c3907c","cacheData":{"env":{}}}